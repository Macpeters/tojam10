            <string>///initialize keys, chord dots submit button
global.scale = get_scale();
global.chord = get_chord();

//chord dots
for (var i = 0; i &lt; array_height_2d(global.chord); i++;){
    dot = instance_create(i * 70 + 100, 60, obj_coloured_dot);
    value = global.chord[i, 0];
    dot.dotColour = value + 200;
    dot.value = value;
    dot.valueModifier = global.chord[i, 1];
}

//submit button
okButton = instance_create(550, 100, obj_global_button);
okButton.buttonLevel = global.BUTTON_OK;

//white keys
whiteKeys[0] = global.C;
whiteKeys[1] = global.D;
whiteKeys[2] = global.E;
whiteKeys[3] = global.F;
whiteKeys[4] = global.G;
whiteKeys[5] = global.A;
whiteKeys[6] = global.B;

for(i = 0; i &lt; global.NUM_WHITE_KEYS; i++){
    dot = instance_create(i * 90 + 20, 380, obj_coloured_dot);
    dot.dotColour = global.WHITE
    dot.value = whiteKeys[i];
    keys_index = get_keys_index(whiteKeys[i]);
    dot.keys_index = get_keys_index(whiteKeys[i]);
    dot.name = get_note_name(whiteKeys[i]);
    dot.valueModifier = 0;
}

//black keys
blackKeys[0] = global.CSHARP;
blackKeys[1] = global.DSHARP;
blackKeys[2] = 0;
blackKeys[3] = global.FSHARP;
blackKeys[4] = global.GSHARP;
blackKeys[5] = global.ASHARP;

for(j = 0; j &lt;= global.NUM_BLACK_KEYS; j++){
    if(j != 2){
        dot = instance_create(j * 90 + 90, 250, obj_coloured_dot);
        dot.dotColour = global.BLACK
        dot.value = blackKeys[j]
        dot.keys_index = get_keys_index(blackKeys[j]);
        dot.name = get_note_name(blackKeys[j]);
        dot.valueModifier = 0;
    }
    
}
</string>
